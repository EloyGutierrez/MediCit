<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/pidetucita"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="134dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/estilos_cajas_de_texto"
        app:layout_constraintBottom_toTopOf="@+id/miscitas"
        app:layout_constraintEnd_toStartOf="@+id/buscatumedico"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pidetucita"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/buscatumedico"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="25dp"
        android:background="@drawable/estilos_cajas_de_texto"
        app:layout_constraintBottom_toTopOf="@+id/consultatuseguro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/pidetucita"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/buscatumedico"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/miscitas"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="43dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="95dp"
        android:background="@drawable/estilos_cajas_de_texto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/consultatuseguro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pidetucita"
        app:srcCompat="@drawable/miscitas"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/consultatuseguro"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="94dp"
        android:background="@drawable/estilos_cajas_de_texto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/miscitas"
        app:layout_constraintTop_toBottomOf="@+id/buscatumedico"
        app:srcCompat="@drawable/consultatuseguro"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="95dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="26dp"
        android:text="Hospital HipÃ³lito Unanue "
        android:textColor="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/buscatumedico"
        app:layout_constraintEnd_toEndOf="@+id/buscatumedico"
        app:layout_constraintStart_toStartOf="@+id/pidetucita"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>