<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imagenid"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="31dp"
        android:layout_marginBottom="60dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toTopOf="@+id/tv_dni"
        app:layout_constraintEnd_toStartOf="@+id/cargarimagen"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_perm_identity_24"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/tv_dni"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="DNI"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/tv_nombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imagenid"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/tv_nombre"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Nombres"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/tv_apellidopaterno"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_dni"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/tv_apellidopaterno"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="21dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Apellido paterno"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/tv_apellidomaterno"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_nombre"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/tv_apellidomaterno"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Apellido materno"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/tv_correo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_apellidopaterno"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/tv_correo"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:textColor="@color/colorPrimary"
        android:textColorHint="@color/colorPrimary"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/tv_celular"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_apellidomaterno"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/tv_celular"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="14dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Telefono"
        android:inputType="number"
        android:textColor="@color/colorPrimary"
        android:textColorHint="@color/colorPrimary"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/btnatras"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_correo"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnactualizar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="62dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:text="Actualizar"
        android:textColor="@color/colorPrimary"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnatras"
        app:layout_constraintTop_toBottomOf="@+id/cargarimagen"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnatras"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:text="ATRAS"
        android:textColor="@color/colorPrimary"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnactualizar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_celular"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/cargarimagen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="101dp"
        android:layout_marginEnd="83dp"
        android:layout_marginBottom="488dp"
        android:clickable="true"
        android:onClick="onclick"
        app:layout_constraintBottom_toTopOf="@+id/btnactualizar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imagenid"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>