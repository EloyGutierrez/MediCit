<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistrarActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="50dp"
                app:srcCompat="@drawable/logomedicit"
                tools:layout_editor_absoluteX="156dp"
                tools:layout_editor_absoluteY="16dp" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="120dp"
                android:layout_marginTop="20dp"
                android:text="Actualizar datos del usuario"
                android:textColor="@color/colorPrimary"
                tools:layout_editor_absoluteX="128dp"
                tools:layout_editor_absoluteY="129dp" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="50dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:text="DNI"
                android:textColor="@color/colorPrimary"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="12dp"
                tools:layout_editor_absoluteY="168dp" />

            <TextView
                android:id="@+id/edtdni"
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:layout_marginLeft="215dp"
                android:layout_marginTop="-40dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Nro. de documento"
                android:inputType="number"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary"
                tools:layout_editor_absoluteX="102dp"
                tools:layout_editor_absoluteY="643dp" />

            <EditText
                android:id="@+id/edtnombre"
                android:layout_width="387dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Nombres"
                android:inputType="textPersonName"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary" />

            <EditText
                android:id="@+id/edtapellidopaterno"
                android:layout_width="387dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Apellido paterno"
                android:inputType="textPersonName"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary"
                tools:ignore="MissingConstraints" />

            <EditText
                android:id="@+id/edtapellidomaterno"
                android:layout_width="387dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Apellido materno"
                android:inputType="textPersonName"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary" />



            <androidx.constraintlayout.widget.Barrier
                android:id="@+id/barrier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:barrierDirection="left"
                tools:layout_editor_absoluteX="411dp" />

            <EditText
                android:id="@+id/edtcorreo"
                android:layout_width="382dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Correo Electronico"
                android:inputType="textPersonName"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary"
                tools:layout_editor_absoluteX="13dp"
                tools:layout_editor_absoluteY="654dp" />

            <EditText
                android:id="@+id/edtcelular"
                android:layout_width="380dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Celular"
                android:inputType="number"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary"
                tools:layout_editor_absoluteX="115dp"
                tools:layout_editor_absoluteY="694dp" />


            <EditText
                android:id="@+id/edtcontrasenia"
                android:layout_width="380dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Contraseña"
                android:inputType="text"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary" />

            <EditText
                android:id="@+id/edtcontraseniaa"
                android:layout_width="380dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Repetir Contraseña"
                android:inputType="text"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary" />


            <Button
                android:id="@+id/btnActualizar"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:onClick="Principal"
                android:text="ACTUALIZAR"
                android:textColor="@color/colorPrimary" />


        </LinearLayout>


    </ScrollView>









</androidx.constraintlayout.widget.ConstraintLayout>