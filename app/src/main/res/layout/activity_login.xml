<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutMode="clipBounds">


    <Button
        android:id="@+id/registrar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/estilos_cajas_de_texto"
        android:onClick="Principal"
        android:text="Iniciar Sesión"
        android:textColor="@color/colorPrimary"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="458dp" />

    <EditText
        android:id="@+id/edtcontraseña"
        android:layout_width="351dp"
        android:layout_height="48dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Contraseña"
        android:inputType="textPassword"
        android:textColor="@color/colorPrimaryDark"
        android:textColorHint="@color/colorPrimary"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="363dp" />

    <EditText
        android:id="@+id/edtusuario"
        android:layout_width="351dp"
        android:layout_height="48dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Nombre de usuario"
        android:inputType="number"
        android:textColor="@color/colorPrimaryDark"
        android:textColorHint="@color/colorPrimary"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="286dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="157dp"
        android:layout_height="127dp"
        app:srcCompat="@mipmap/logomedicit"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="76dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="184dp"
        android:layout_height="36dp"
        android:onClick="Olvidemicontraseña"
        android:text="¿Olvidé mi contraseña?"
        android:textAlignment="center"
        android:textColor="@color/colorPrimary"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="522dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="Registrar"
        android:text="Regístrate como paciente"
        android:textColor="@color/colorPrimary"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="572dp" />
    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="Registrarmedico"
        android:text="Regístrate como médico"
        android:textColor="@color/colorPrimary"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="626dp"
        tools:ignore="MissingConstraints" />

    <include
        android:id="@+id/include6"
        layout="@layout/content_main"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="45dp" />



</androidx.constraintlayout.widget.ConstraintLayout>