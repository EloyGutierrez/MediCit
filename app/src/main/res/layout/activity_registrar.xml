<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegistrarActivity">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginTop="50dp"
                android:layout_marginLeft="150dp"
                app:srcCompat="@drawable/logomedicit"
                tools:layout_editor_absoluteX="156dp"
                tools:layout_editor_absoluteY="16dp" />
            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="120dp"
                android:text="Hospital Hipólito Unanue"
                android:textColor="@color/colorPrimary"
                tools:layout_editor_absoluteX="128dp"
                tools:layout_editor_absoluteY="129dp" />
            <TextView
                android:id="@+id/textView19"
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:text="DNI"
                android:textColor="@color/colorPrimary"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="50dp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="12dp"
                tools:layout_editor_absoluteY="168dp" />
            <EditText
                android:id="@+id/edtdni"
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Nro. de documento"
                android:textColor="@color/colorPrimary"
                android:layout_marginLeft="215dp"
                android:layout_marginTop="-40dp"
                android:inputType="number"
                android:textColorHint="@color/colorPrimary"
                tools:layout_editor_absoluteX="102dp"
                tools:layout_editor_absoluteY="643dp" />

            <EditText
                android:id="@+id/edtnombre"
                android:layout_width="387dp"
                android:layout_height="40dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Nombres"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="10dp"
                android:inputType="textPersonName"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary" />

            <EditText
                android:id="@+id/edtapellidopaterno"
                android:layout_width="387dp"
                android:layout_height="40dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Apellido paterno"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="10dp"
                android:inputType="textPersonName"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary"
                tools:ignore="MissingConstraints" />
            <EditText
                android:id="@+id/edtapellidomaterno"
                android:layout_width="387dp"
                android:layout_height="40dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Apellido materno"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp"
                android:inputType="textPersonName"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sexo"
                android:textColor="@color/colorPrimary"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="603dp" />
            <Spinner
                android:id="@+id/idspinnerSexo"
                android:layout_width="380dp"
                android:layout_height="40dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/estilos_cajas_de_texto"
                tools:layout_editor_absoluteX="193dp"
                tools:layout_editor_absoluteY="621dp" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fecha de nacimiento"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp"
                android:textColor="@color/colorPrimary"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="25dp"
                tools:layout_editor_absoluteY="587dp" />

            <EditText
                android:id="@+id/edtfechadenacimiento"
                android:layout_width="380dp"
                android:layout_height="40dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:hint="fecha de nacimiento"
                android:background="@drawable/estilos_cajas_de_texto"
                android:textColorHint="@color/colorPrimary"
                android:textColor="@color/colorPrimary"
                android:ems="10"
                android:inputType="date" />




            <androidx.constraintlayout.widget.Barrier
                android:id="@+id/barrier"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:barrierDirection="left"
                tools:layout_editor_absoluteX="411dp" />

            <EditText
                android:id="@+id/edtcorreo"
                android:layout_width="382dp"
                android:layout_height="40dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp"
                android:hint="Correo Electronico"
                android:inputType="textPersonName"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary"
                tools:layout_editor_absoluteX="13dp"
                tools:layout_editor_absoluteY="654dp" />

            <EditText
                android:id="@+id/edtcelular"
                android:layout_width="380dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:ems="10"
                android:hint="Celular"
                android:textColor="@color/colorPrimary"
                android:inputType="number"
                android:textColorHint="@color/colorPrimary"
                tools:layout_editor_absoluteX="115dp"
                tools:layout_editor_absoluteY="694dp" />

            <TextView
                android:id="@+id/textView265"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Seguro / Plan de Salud"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="10dp"
                android:textColor="@color/colorPrimary"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="25dp"
                tools:layout_editor_absoluteY="587dp" />

            <Spinner
                android:id="@+id/idspinnerseguro"
                android:layout_width="380dp"
                android:layout_height="40dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/estilos_cajas_de_texto"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="467dp" />

            <EditText
                android:id="@+id/edtcontrasenia"
                android:layout_width="380dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:textColorHint="@color/colorPrimary"
                android:background="@drawable/estilos_cajas_de_texto"
                android:textColor="@color/colorPrimary"
                android:hint="Contraseña"
                android:ems="10"
                android:inputType="textPassword" />
            <EditText
                android:id="@+id/edtcontraseniaa"
                android:layout_width="380dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:hint="Repetir Contraseña"
                android:textColor="@color/colorPrimary"
                android:textColorHint="@color/colorPrimary"
                android:ems="10"
                android:inputType="textPassword" />


            <Button
                android:id="@+id/btnregistrar"
                android:layout_width="200dp"
                android:layout_height="40dp"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/estilos_cajas_de_texto"
                android:onClick="Principal"
                android:text="REGISTRAR"
                android:textColor="@color/colorPrimary" />


        </LinearLayout>


    </ScrollView>









</androidx.constraintlayout.widget.ConstraintLayout>