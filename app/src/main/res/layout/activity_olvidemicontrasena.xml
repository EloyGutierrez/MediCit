<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/estilos_cajas_de_texto"
        android:text="REGRESAR"
        android:textColor="@color/colorPrimary"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="589dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/estilos_cajas_de_texto"
        android:text="CONTINUAR"
        android:textColor="@color/colorPrimary"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="242dp"
        tools:layout_editor_absoluteY="589dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="100dp"
        android:layout_height="81dp"
        app:srcCompat="@drawable/logomedicit"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="27dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OLVIDÉ MI CONTRASEÑA"
        android:textColor="@color/colorPrimary"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="152dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="363dp"
        android:layout_height="81dp"
        android:justificationMode="inter_word"
        android:text="No te preocupes. Te enviaremos una nueva contraseña al buzon que indiques a continuación."
        android:textAlignment="center"
        android:textColor="@color/colorPrimary"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="193dp" />

</androidx.constraintlayout.widget.ConstraintLayout>