<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="161dp"
        android:layout_height="119dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="11dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.046"
        app:srcCompat="@drawable/logomedicit" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="280dp"
        android:layout_height="44dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:hint="Usuario"
        android:textColor="@color/colorPrimary"
        android:textColorHint="@color/colorPrimary"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="190dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="280dp"
        android:layout_height="44dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:hint="Cita médica"
        android:textColorHint="@color/colorPrimary"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="266dp" />

    <EditText
        android:id="@+id/editTextNumber3"
        android:layout_width="280dp"
        android:layout_height="44dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Número de targeta"
        android:inputType="number"
        android:textColor="@color/colorPrimary"
        android:textColorHint="@color/colorPrimary"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="330dp" />

    <EditText
        android:id="@+id/editTextNumber4"
        android:layout_width="150dp"
        android:layout_height="44dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="      CVV"
        android:inputType="number"
        android:textColor="@color/colorPrimary"
        android:textColorHint="@color/colorPrimary"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="398dp" />

    <EditText
        android:id="@+id/editTextDate"
        android:layout_width="190dp"
        android:layout_height="44dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Fecha venc."
        android:inputType="date"
        android:textColor="@color/colorPrimary"
        android:textColorHint="@color/colorPrimary"
        tools:layout_editor_absoluteX="205dp"
        tools:layout_editor_absoluteY="398dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="150dp"
        android:layout_height="44dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:textColorHint="@color/colorPrimary"
        android:hint="Nombre"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="459dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="190dp"
        android:layout_height="44dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:hint="Apellidos"
        android:textColor="@color/colorPrimary"
        android:textColorHint="@color/colorPrimary"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="205dp"
        tools:layout_editor_absoluteY="466dp" />

    <EditText
        android:id="@+id/editTextNumber5"
        android:layout_width="280dp"
        android:layout_height="44dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:textColorHint="@color/colorPrimary"
        android:textColor="@color/colorPrimary"
        android:hint="DNI"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="522dp" />

    <Button
        android:id="@+id/btnCContinuar"
        android:layout_width="125dp"
        android:layout_height="41dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:text="Pagar"
        android:textColor="@color/colorPrimary"
        android:textSize="19dp"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="588dp" />

</androidx.constraintlayout.widget.ConstraintLayout>