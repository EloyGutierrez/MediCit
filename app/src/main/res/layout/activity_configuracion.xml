<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="184dp"
        android:layout_height="0dp"
        android:layout_marginTop="28dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logomedicit"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="160dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toTopOf="@+id/edtdni"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:srcCompat="@drawable/ic_baseline_perm_identity_24"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtdni"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="DNI"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/edtnombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtnombre"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Nombres"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/edtapellidopaterno"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtdni"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtapellidopaterno"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Apellido paterno"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/edtapellidomaterno"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtnombre"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtapellidomaterno"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Apellido materno"
        android:inputType="textPersonName"
        android:textColorHint="@color/colorPrimary"
        app:layout_constraintBottom_toTopOf="@+id/edtcorreo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtapellidopaterno"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtcorreo"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:textColor="@color/colorPrimary"
        android:textColorHint="@color/colorPrimary"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/edtcelular"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtapellidomaterno"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edtcelular"
        android:layout_width="230dp"
        android:layout_height="0dp"
        android:layout_marginBottom="14dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:ems="10"
        android:hint="Telefono"
        android:inputType="number"
        android:textColor="@color/colorPrimary"
        android:textColorHint="@color/colorPrimary"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/btnactualizar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtcorreo"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnactualizar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="62dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:text="Actualizar"
        android:textColor="@color/colorPrimary"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnatras"
        app:layout_constraintTop_toBottomOf="@+id/edtcelular"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnatras"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="67dp"
        android:layout_marginEnd="22dp"
        android:background="@drawable/estilos_cajas_de_texto"
        android:text="ATRAS"
        android:textColor="@color/colorPrimary"
        android:textSize="18dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/btnactualizar"
        app:layout_constraintEnd_toStartOf="@+id/btnactualizar"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>